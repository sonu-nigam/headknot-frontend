{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true,
            "dependsOn": ["^dev"]
        },
        "build:schema": {
            "outputs": []
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "dist/**", "build/**"]
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "outputs": []
        },
        "typecheck": {
            "outputs": []
        }
    }
}
